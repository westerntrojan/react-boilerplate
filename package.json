		"lint:css": "stylelint ./src/**/*.{css,scss} --config ./extras/.stylelintrc.json",
		"lint": "yarn lint:js && yarn lint:css",
		"prettify": "prettier ./src/**/*.{js,jsx,ts,tsx} --write"
	},
